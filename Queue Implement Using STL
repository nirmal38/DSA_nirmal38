#include<bits/stdc++.h>
using namespace std;
  int main()
{
  //.......Queue..........
  queue<int>q;
  q.push(11);
  cout<<"Front of queue is:"<<q.front()<<endl;
  q.push(12);
  cout<<"Front of queue is:"<<q.front()<<endl;
  q.push(13);
  cout<<"Front of queue is:"<<q.front()<<endl;

  cout<<"Size of queue is :"<<q.size()<<endl;

  q.pop();
  q.pop();
  q.pop();

  cout<<"Size of queue is :"<<q.size()<<endl;
  if(q.empty()){
    cout<<"Queue is empty"<<endl;

  }
  else{
    cout<<"Queue is not empty"<<endl;
  }

  // ..........DeQueue...............
   deque<int>d;
  d.push_front(11);
  d.push_back(12);
  cout<<d.front()<<endl;
  cout<<d.back()<<endl;

  d.pop_front();
  cout<<d.front()<<endl;
  cout<<d.back()<<endl;

  d.pop_back();
  
  if(d.empty()){
    cout<<"Queue is empty"<<endl;

  }
  else{
    cout<<"Queue is not empty"<<endl;
  }

  return 0;
}

